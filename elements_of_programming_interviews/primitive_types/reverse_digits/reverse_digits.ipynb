    "%%writefile reverse_digits.py\n",
    "# O(n) time | O(n) space\n",
    "def reverse_digtis(num):\n",
    "    result, num_remaining = 0, abs(num_remaining)\n",
    "    while num_remaining:\n",
    "        result = result * 10 + num_remaining % 10\n",
    "        num_remaining //= 10\n",
    "    return -result if num < 0 else num\n"
   ]
